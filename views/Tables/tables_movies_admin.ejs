  <%- contentFor('body') %>
  
  <!-- Start content -->
                <div class="content">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="page-title-box">
                                  <form action="/add_movie" method ="get">
                                    
                                      
                                        <button type="submit"  style="margin-right:50px;"
                                        class="btn btn-info">Add New Movie </button>
                                     
                                        <button type="button"  style="margin-right:50px;" onclick="location.href='/logout'"
                                            class="btn btn-warning float-right">Logout </button>
                                       
                                    </div>
                                   
                                </div>
                             
                            </div>
                          
                        </div>
                        <!-- end row -->
                      
                        <div class="row">
                            <div class="col-12">
                                <div class="card m-b-20">
                                    <div class="card-body">
                                        <h4 class="mt-0 header-title">Movies List</h4>
                                        
                                        
                                        <table  id="datatable-buttons" class="table table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                                            <thead>
                                                <tr>
                                                    <th> Name </th>
                                                    <th>Director</th>
                                                    <th>Imdb Score</th>
                                                    <th>Edit</th>
                                                    <th>Delete</th>
                                                </tr>
                                                </thead>
                                            <tbody>
                                                <% for(var i=0; i < data.length; i++) { %>
                                                    <tr>
                                                        <td><%= data[i].name %></td>
                                                        <td><%= data[i].director %></td>
                                                        <td><%= data[i].imdb_score %></td>
                                                        <td> <button type="button"  style="margin-right:50px;" onclick="location.href='/delete?id=<%= data[i]._id %>'"
                                                            class="btn btn-danger">Delete </button></td>
                                                        <td> <button type="button"  style="margin-right:50px;" onclick="location.href='/edit?id=<%= data[i]._id %>'"
                                                            class="btn btn-primary">Edit </button></td>
                                                    </tr>
                                                 <% } %>
                                            </tbody>
                                        </table>

                                    </div>
                                </div>
                            </div> <!-- end col -->
                        </div> <!-- end row -->
                    </div> <!-- container-fluid -->

                </div> <!-- content -->
            </form>

